{
    "description": " A mostly non-sensical test which simply exercises the parser checking for anomalous behavior.  This test should not provoke any errors, provided that counting-5.txt does not exist in its reaction output directory. (This is to test the \"no overwrite\" output mode for counting.) This variant produces DREAMM grouped style output, and specifies output times using TIME_POINTS, exercising all of the wildcard matching variantsin the VIZ output blocks.",
    "author": "Jed Wing <jed@salk.edu>",
    "date": "2008-04-04",
    "keywords": [
        "parser"
    ],
    "run": {
        "mdlfiles": [
            "test.mdl"
        ],
        "commandlineOpts": [
            "-iterations",
            "100"
        ]
    },
    "checks": [
        {
            "testType": "CHECK_SUCCESS"
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 5",
            "dataFile": "dat/counting-1.txt",
            "haveHeader": false,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                5
            ],
            "countMaximum": [
                5
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 6. Exact time turned off -- should have no effect on counts",
            "dataFile": "dat/counting-2.txt",
            "haveHeader": false,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                6
            ],
            "countMaximum": [
                6
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 7. Header explicitly turned off -- should have no effect on counts.",
            "dataFile": "dat/counting-3.txt",
            "haveHeader": false,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                7
            ],
            "countMaximum": [
                7
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 8. Exact time turned on -- should have no effect on counts.",
            "dataFile": "dat/counting-4.txt",
            "haveHeader": false,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                8
            ],
            "countMaximum": [
                8
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 9. Header turned off (YES/NO syntax, rather than NONE).",
            "dataFile": "dat/counting-5.txt",
            "haveHeader": false,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                9
            ],
            "countMaximum": [
                9
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant count of 10. Header turned but on but no custom header given.",
            "dataFile": "dat/counting-6.txt",
            "haveHeader": true,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                10
            ],
            "countMaximum": [
                10
            ]
        },
        {
            "testType": "COUNT_MINMAX",
            "description": "Constant counts of 11, 12, 13. Custom header 'Duck, Duck, Goose' given.",
            "dataFile": "dat/counting-7.txt",
            "haveHeader": true,
            "minTime": 0.0,
            "maxTime": 1e-4,
            "countMinimum": [
                11,
                12,
                13
            ],
            "countMaximum": [
                11,
                12,
                13
            ]
        },
        {
            "testType": "CHECK_TRIGGERS",
            "description": "Trigger for count of s_v_4 in world.big_object.newbox. Could try to validate against counting-8.txt. Exact time is left at default (ON).",
            "dataFile": "dat/counting-19.txt",
            "triggerType": "molCounts",
            "outputTime": 1e-6,
            "haveExactTime": true,
            "xrange": [
                -20,
                20
            ],
            "yrange": [
                -20,
                20
            ],
            "zrange": [
                -20,
                20
            ]
        },
        {
            "testType": "CHECK_TRIGGERS",
            "description": "Trigger for count of s_v_4 in world.big_object.newbox. Could try to validate against counting-8.txt. Exact time is explicitly turned OFF.",
            "dataFile": "dat/counting-20.txt",
            "triggerType": "molCounts",
            "outputTime": 1e-6,
            "haveExactTime": false,
            "xrange": [
                -20,
                20
            ],
            "yrange": [
                -20,
                20
            ],
            "zrange": [
                -20,
                20
            ]
        },
        {
            "testType": "CHECK_TRIGGERS",
            "description": "Trigger for count of s_v_0 in world.big_object.newbox. Could try to validate against counting-8.txt  Exact time is explicitly turned ON.",
            "dataFile": "dat/counting-21.txt",
            "triggerType": "molCounts",
            "outputTime": 1e-6,
            "haveExactTime": true,
            "xrange": [
                -20,
                20
            ],
            "yrange": [
                -20,
                20
            ],
            "zrange": [
                -20,
                20
            ]
        },
        {
            "testType": "CHECK_NONEMPTY_FILES",
            "fileNames": [
                "dat/counting-%d.txt"
            ],
            "IDRange": [
                "1:21"
            ]
        },
        {
            "testType": "CHECK_NONEMPTY_FILES",
            "fileNames": [
                "vol_dat/vo.%d.dat"
            ],
            "IDrange": [
                "0",
                "100"
            ]
        },
        {
            "testType": "CHECK_NONEMPTY_FILES",
            "fileNames": [
                "vol_dat/ovo.%d.dat"
            ],
            "IDRange": [
                "1",
                "10",
                "100"
            ]
        },
        {
            "testType": "CHECK_NONEMPTY_FILES",
            "fileNames": [
                "vol_dat/nvo.100.dat",
                "parser_viz_dreammgrouped_wildcard-expressions.dat",
                "parser_viz_dreammgrouped_wildcard.dat",
                "run_%d.0.log",
                "err_%d.0.log"
            ]
        },
        {
            "testType": "FILE_MATCH_PATTERN",
            "dataFile": "err_%d.0.log",
            "matchPattern": "Warning: Could not release 6 of s_g_1 \\(surface full\\).\n",
            "numMatches": 1
        },
        {
            "testType": "CHECK_EXPRESSIONS",
            "dataFile": "parser_viz_dreammgrouped_wildcard-expressions.dat"
        },
        {
            "testType": "DIFF_FILE_CONTENT",
            "dataFile": "parser_viz_dreammgrouped_wildcard.dat",
            "templateFile": "parser_viz_dreammgrouped_wildcard_template.dat",
            "templateParameters": [
                "TODAY_DAY"
            ]
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/vo.0.dat",
            "Xdim": 25,
            "Ydim": 25,
            "Zdim": 25
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/vo.100.dat",
            "Xdim": 25,
            "Ydim": 25,
            "Zdim": 25
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/ovo.1.dat",
            "Xdim": 1,
            "Ydim": 2,
            "Zdim": 4
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/ovo.10.dat",
            "Xdim": 1,
            "Ydim": 2,
            "Zdim": 4
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/ovo.100.dat",
            "Xdim": 1,
            "Ydim": 2,
            "Zdim": 4
        },
        {
            "testType": "CHECK_LEGACY_VOL_OUTPUT",
            "dataFile": "vol_dat/nvo.100.dat",
            "Xdim": 1,
            "Ydim": 1,
            "Zdim": 1
        },
        {
            "testType": "CHECK_DREAMM_V3_GROUPED",
            "vizPath": "viz_dat/world",
            "numIters": 101,
            "haveMeshPos" : true,
            "haveRgnIdx" : true,
            "haveMolPos" : true,
            "haveMolOrient": true,
            "haveMolState": true,
            "noMols" : true
       },
       {
            "testType": "CHECK_DREAMM_V3_GROUPED",
            "vizPath": "viz_dat/world2",
            "numIters": 101,
            "haveMeshPos" : true,
            "haveRgnIdx" : true,
            "haveMolPos" : true,
            "haveMolOrient": true
       }
    ]
}
