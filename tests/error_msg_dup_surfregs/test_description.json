{
  "description": "Verify that an error is generated when trying to define two surface regions with the same name.",
  "author": "Jacob Czech",
  "date": "2016-01-08",
  "keywords": [
    "error messages"
  ],
  "run": {
      "mdlfiles": ["error.mdl"],
      "commandlineOpts": [
        "-quiet"
      ]
  },
  "checks": [
    {
       "testType": "FILE_MATCH_PATTERN",
       "dataFile": "err_%d.0.log",
       "matchPattern": "Region already defined: A,sr",
       "numMatches": 1
    }
  ]
}
