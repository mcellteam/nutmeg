ITERATIONS = 200
TIME_STEP = 1e-6

WARNINGS
{
  LARGE_MOLECULAR_DISPLACEMENT = IGNORED
}

DYNAMIC_GEOMETRY = "./list.txt"

DEFINE_MOLECULES
{
  sm { DIFFUSION_CONSTANT_2D = 1e-08 }
  vm { DIFFUSION_CONSTANT_3D = 1e-08 }
}

DEFINE_RELEASE_PATTERN rel_pat
{
  DELAY = 100E-6
  RELEASE_INTERVAL = 10E-6
  TRAIN_DURATION = 20E-6
  TRAIN_INTERVAL = 30E-6
  NUMBER_OF_TRAINS = 3
}

INSTANTIATE Scene OBJECT
{
  rel_sm RELEASE_SITE
  {
    SHAPE = DGScene.Cube
    MOLECULE = sm'
    NUMBER_TO_RELEASE = 10
    RELEASE_PATTERN = rel_pat
  }
  rel_vm RELEASE_SITE
  {
    SHAPE = DGScene.Cube
    MOLECULE = vm
    NUMBER_TO_RELEASE = 10
    RELEASE_PATTERN = rel_pat
  }
}

REACTION_DATA_OUTPUT
{
  STEP=1e-06
  {COUNT[sm,WORLD]}=> "./sm.WORLD.dat"
  {COUNT[vm,WORLD]}=> "./vm.WORLD.dat"
}
