ITERATIONS = 15
TIME_STEP = 1e-6

WARNINGS
{
  LARGE_MOLECULAR_DISPLACEMENT = IGNORED
}

DYNAMIC_GEOMETRY = "./geom.txt"

DEFINE_MOLECULES
{
  vm_1 { DIFFUSION_CONSTANT_3D = 0 }
}

INCLUDE_FILE = "./geom1.mdl"

INSTANTIATE Scene OBJECT
{
  A OBJECT A{}
  B OBJECT B{}
  C OBJECT C{}
  rel_vm_1 RELEASE_SITE
  {
    SHAPE = Scene.A
    MOLECULE = vm_1
    NUMBER_TO_RELEASE = 1000
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
}

REACTION_DATA_OUTPUT
{
  ITERATION_LIST = [[0 TO 15 STEP 1]]
  {COUNT[vm_1,WORLD]}=> "./vm_1_world.dat"
  {COUNT[vm_1,Scene.A]}=> "./vm_1_A.dat"
  {COUNT[vm_1,Scene.B]}=> "./vm_1_B.dat"
  {COUNT[vm_1,Scene.C]}=> "./vm_1_C.dat"
  {COUNT[vm_1,Scene.D]}=> "./vm_1_D.dat"
}
